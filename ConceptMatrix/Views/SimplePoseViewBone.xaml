<UserControl x:Class="ConceptMatrix.Views.SimplePoseViewBone"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:ConceptMatrix.Views"
			 DataContextChanged="OnDataContextChanged"
			 mc:Ignorable="d" 
			 Width="18"
			 Height="18"
			 MouseEnter="OnMouseEnter"
			 MouseLeave="OnMouseLeave"
			 MouseUp="OnMouseUp">
	<Grid>
		<Ellipse Stroke="Gray" StrokeThickness="1" Fill="#404040" x:Name="Background"/>
		<Ellipse Fill="Gray" Margin="3" x:Name="Foreground" Visibility="Hidden"/>
		<!--<RadioButton x:Name="Button" Checked="OnChecked" MouseEnter="OnMouseEnter" MouseLeave="OnMouseLeave"/>-->
	</Grid>
</UserControl>
